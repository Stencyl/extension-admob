<xml>

<pragma once="true" />

<set name="id" value="com.stencyl.admob" />

<error value="The ${id} extension only provides native code for iOS" unless="ios" />

<files id="${id}-dependencies">
  <depend files="hxcpp-depends" />
  <depend name="${this_dir}/Build.xml" dateOnly="true" />
  <depend name="${this_dir}/include/AdMobEx.h" />
  <depend name="${this_dir}/include/PACConsentForm.h" />
  <depend name="${this_dir}/include/PACError.h" />
  <depend name="${this_dir}/include/PACPersonalizedAdConsent.h" />
  <depend name="${this_dir}/include/PACView.h" />
</files>

<files id="${id}-common" dir="${this_dir}">
  <depend files="${id}-dependencies" />
  <cache value="1" project="${id}" asLibrary="true" />
  <compilerflag value="-I${this_dir}/include" />
  <file name="common/ExternalInterface.cpp" />
</files>

<files id="${id}-ios" dir="${this_dir}">
  <depend files="${id}-dependencies" />
  <cache value="1" project="${id}" asLibrary="true" />
  <compilerflag value="-I${this_dir}/include" />
  <compilerflag value="-F${this_dir}/../frameworks/" />
  <compilerflag name="-framework" value="GoogleMobileAds" />
  <file name="ios/AdMobEx.mm" />
  <file name="ios/PACConsentForm.m" />
  <file name="ios/PACError.m" />
  <file name="ios/PACPersonalizedAdConsent.m" />
  <file name="ios/PACView.m" />
</files>

<target id="haxe">
  <files id="com.stencyl.admob-common" />
  <files id="com.stencyl.admob-ios" if="ios" />
</target>

<unset name="id" />

</xml>